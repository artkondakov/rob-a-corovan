{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 3 versions",
            "ie 11"
          ]
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "throwIfNamespace": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "throwIfNamespace": false
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}